  "scripts": {
    "prebuild": "rm -rf ./dist && ts-node ./src/scripts/generateSitemap.ts",
    "build": "NODE_ENV=production yarn webpack --config webpack/webpack.prod.js",
    "start:prod:server": "yarn http-server dist -p 4040",
    "start:prod": "yarn build && yarn start:prod:server",
    "start:dev": "yarn webpack serve --config webpack/webpack.dev.js",
    "tests:jest": "NODE_ENV=production jest",
    "tests:jest:update": "yarn tests:jest -u",
    "tests:pw:build": "docker-compose build --no-cache pg-tests-runner",
    "tests:pw:run": "docker-compose run pg-tests-runner",
    "tests:pw": "yarn tests:pw:run yarn build && yarn tests:pw:run",
    "tests:pw:update": "yarn tests:pw:run yarn build && yarn tests:pw:run yarn playwright test -u",
    "tests": "yarn tests:jest && yarn tests:pw",
    "prettify": "yarn prettier '**/*.{cjs,js,jsx,ts,tsx,css,scss,sass,yml,json}' --check",
    "prettify:fix": "yarn prettify --write",
    "lint:ts": "tsc --noEmit && eslint . --ext .cjs,.js,.jsx,.ts,.tsx",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:css": "stylelint ./src/**/*.{scss,sass,css}",
    "lint:css:fix": "yarn lint:css --fix",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:fix": "yarn lint:ts:fix && yarn lint:css:fix",
    "upgrade:all": "yarn-upgrade-all",
    "upgrade:latest": "yarn upgrade --latest"
  }
