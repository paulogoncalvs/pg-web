  "scripts": {
    "build": "rm -rf ./dist && NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "start:prod:server": "http-server dist -p 4040",
    "start:prod": "yarn build && yarn start:prod:server",
    "start:dev": "webpack serve --config webpack/webpack.dev.js",
    "tests:jest": "jest",
    "tests:jest:update": "jest -u",
    "tests:playwright:build": "docker-compose build --no-cache pg-tests-runner",
    "tests:playwright": "yarn build && docker-compose run pg-tests-runner",
    "tests:playwright:run": "docker-compose run pg-tests-runner",
    "tests:playwright:update": "yarn build && docker-compose run pg-tests-runner yarn playwright test -u",
    "tests": "yarn tests:jest && yarn tests:playwright",
    "prettify": "yarn prettier \"**/*.*(js|jsx|ts|tsx)\" --ignore-path=.prettierignore --write",
    "lint:ts": "tsc --noEmit && eslint src --ext .js,.jsx,.ts,.tsx && yarn prettify",
    "lint:ts-fix": "tsc --noEmit && eslint src --ext .js,.jsx,.ts,.tsx --fix && yarn prettify",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint:css-fix": "stylelint ./src/**/*.scss --fix",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:fix": "yarn lint:ts-fix && yarn lint:css-fix",
    "upgrade:all": "yarn-upgrade-all",
    "upgrade:latest": "yarn upgrade --latest"
